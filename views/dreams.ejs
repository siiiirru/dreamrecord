<body>
    <header id="chathead">
        <span id="username">
            <%=name %>의
        </span>
        <span id="drm">꿈</span>
        <span id="wri">기록장</span>
    </header>
    <button onclick=history.back(); type="button" id="goback">캘린더로 돌아가기</button>
    <div id="dcha">
        <div id="imgbox">
            <img src="data:image/jpeg;base64,<%=sdi%>" id="simg">
        </div>
        <div id="low">
            <div id="namebox">
                <h2 id="dreamname">
                    <%=sdq%>
                </h2>
            </div>
            <div id="answer">
                <h2 id="dreamcontent">
                    <%=sda.replace(/<br>/g, "\n")%>
                </h2>
            </div>
        </div>
    </div>
    </div>

